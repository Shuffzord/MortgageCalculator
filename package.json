    "validate": "node scripts/pre-build-checks.js",
    "precommit": "npm run validate && npm run test",
    "predeploy": "npm run validate && npm run build",
    "deploy:azure": "npm run build && pwsh -NoProfile -File ./scripts/deploy-to-azure.ps1"
