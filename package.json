{
  "name": "smarter-loan",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/pre-build-checks.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "serve -s dist",
    "prepare": "husky",
    "check": "tsc",
    "deploy:local": "npm run build && swa start dist/public --api-location \"\"",
    "fulltest": "npm test && npm run test:e2e",
    "test": "jest --config jest.config.cjs --maxWorkers=80%",
    "test:failed": "jest --onlyFailures --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "ts-node client/e2e-tests/run-tests.ts",
    "test:e2e:report": "ts-node client/e2e-tests/run-tests.ts --report",
    "security-audit": "node scripts/security-audit.js",
    "check-logs": "node scripts/check-console-logs.js",
    "check:translations": "node scripts/translation-validator.js",
    "check:translations:report": "node scripts/translation-validator.js --format markdown --output claudedocs/translation-report.md",
    "check:translations:ci": "node scripts/translation-validator.js --strict",
    "validate": "node scripts/pre-build-checks.js",
    "precommit": "npm run validate && npm run test",
    "predeploy": "npm run validate && npm run build",
    "deploy:azure": "npm run build && pwsh -NoProfile -File ./scripts/deploy-to-azure.ps1",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "eslint . --ext .ts,.tsx --quiet --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
